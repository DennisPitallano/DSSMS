@using AJ3.Core.Data.Entity
@model StudentApplicationFormViewModel
@{
    ViewData["Title"] = "New";
    var comProf = (CompanyProfile)ViewBag.CompanyProfile;
}

@section style
{
    <!--begin::Page Custom Styles(used by this page)-->
    <link href="assets/css/pages/wizard/wizard-4.css" rel="stylesheet" type="text/css" />
    <!--end::Page Custom Styles-->
}

<!--begin::Content-->
<div class="content d-flex flex-column py-lg-3 flex-column-fluid" id="kt_content">
    <!--begin::Subheader-->
    <div class="subheader py-3 py-lg-2 subheader-transparent" id="kt_subheader">
        <div class="container d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
            <!--begin::Details-->
            <div class="d-flex align-items-center flex-wrap mr-2">
                <!--begin::Title-->
                <h5 class="text-dark font-weight-bold mt-2 mb-2 mr-5">New Student</h5>
                <!--end::Title-->
                <!--begin::Separator-->
                <div class="subheader-separator subheader-separator-ver mt-2 mb-2 mr-5 bg-gray-200"></div>
                <!--end::Separator-->
                <!--begin::Search Form-->
                <div class="d-flex align-items-center" id="kt_subheader_search">
                    <span class="text-dark-50 font-weight-bold" id="kt_subheader_total">Enter student details and course</span>
                </div>
                <!--end::Search Form-->
            </div>
            <!--end::Details-->
            <!--begin::Toolbar-->
            <div class="d-flex align-items-center mr-2">
                <!--begin::Button-->
                <a href="#" class="btn btn-default font-weight-bold btn-sm px-3 font-size-base">Cancel</a>
                <!--end::Button-->
                <!--begin::Dropdown-->
            </div>
            <!--end::Toolbar-->
        </div>
    </div>
    <!--end::Subheader-->
    <!--begin::Entry-->
    <div class="d-flex flex-column-fluid">
        <!--begin::Container-->
        <div class="container">
            <!--begin::Card-->
            <div class="card card-custom card-transparent">
                <div class="card-body p-0">
                    <!--begin::Wizard-->
                    <div class="wizard wizard-4" id="kt_wizard" data-wizard-state="step-first" data-wizard-clickable="true">
                        <!--begin::Wizard Nav-->
                        <div class="wizard-nav">
                            <div class="wizard-steps">
                                <div class="wizard-step" data-wizard-type="step" data-wizard-state="current">
                                    <div class="wizard-wrapper">
                                        <div class="wizard-number">1</div>
                                        <div class="wizard-label">
                                            <div class="wizard-title">Profile</div>
                                            <div class="wizard-desc">Student's Personal Information</div>
                                        </div>
                                    </div>
                                </div>
                                <div class="wizard-step" data-wizard-type="step">
                                    <div class="wizard-wrapper">
                                        <div class="wizard-number">2</div>
                                        <div class="wizard-label">
                                            <div class="wizard-title">Course</div>
                                            <div class="wizard-desc">Select Course</div>
                                        </div>
                                    </div>
                                </div>
                                <div class="wizard-step" data-wizard-type="step">
                                    <div class="wizard-wrapper">
                                        <div class="wizard-number">3</div>
                                        <div class="wizard-label">
                                            <div class="wizard-title">Payment</div>
                                            <div class="wizard-desc">Initial or full payment</div>
                                        </div>
                                    </div>
                                </div>
                                <div class="wizard-step" data-wizard-type="step">
                                    <div class="wizard-wrapper">
                                        <div class="wizard-number">4</div>
                                        <div class="wizard-label">
                                            <div class="wizard-title">Summary</div>
                                            <div class="wizard-desc">Review and Submit</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--end::Wizard Nav-->
                        <!--begin::Card-->
                        <div class="card card-custom card-shadowless rounded-top-0">
                            <!--begin::Body-->
                            <div class="card-body p-0">
                                <div class="row justify-content-center py-8 px-8 py-lg-15 px-lg-10">
                                    <div class="col-xl-12 col-xxl-10">
                                        <!--begin::Wizard Form-->
                                        <form class="form" id="kt_form" asp-action="SaveStudent" method="post">
                                            <input type="hidden" asp-for="CourseId" />
                                            <input type="hidden" asp-for="Id" />
                                            <input type="hidden" asp-for="TotalAmount" />
                                            <input type="hidden" asp-for="TotalDiscount" />
                                            <input type="hidden" asp-for="TotalHours" />
                                            <input type="hidden" asp-for="OrderNumber" />
                                            <input type="hidden" asp-for="PayAmount" />
                                            <div class="row justify-content-center">
                                            <div class="col-xl-10">
                                                <!--begin::Wizard Step 1-->
                                                <div class="my-5 step" data-wizard-type="step-content" data-wizard-state="current">
                                                    <h5 class="text-dark font-weight-bold mb-10">Student's Profile Details:</h5>
                                                    <!--begin::Group-->
                                                    @*<div class="form-group row">
                                                            <label class="col-xl-3 col-lg-3 col-form-label text-left">Picture</label>
                                                            <div class="col-lg-9 col-xl-9">
                                                            <div class="image-input image-input-outline image-input-circle" id="kt_user_add_avatar">
                                                            <div class="image-input-wrapper" style="background-image: url(assets/media/users/blank.png)"></div>
                                                            <label class="btn btn-xs btn-icon btn-circle btn-white btn-hover-text-primary btn-shadow" data-action="change" data-toggle="tooltip" title="" data-original-title="Change avatar">
                                                            <i class="fa fa-pen icon-sm text-muted"></i>
                                                            <input type="file" name="profile_avatar" accept=".png, .jpg, .jpeg" />
                                                            <input type="hidden" name="profile_avatar_remove" />
                                                            </label>
                                                            <span class="btn btn-xs btn-icon btn-circle btn-white btn-hover-text-primary btn-shadow" data-action="cancel" data-toggle="tooltip" title="Cancel avatar">
                                                            <i class="ki ki-bold-close icon-xs text-muted"></i>
                                                            </span>
                                                            </div>
                                                            </div>
                                                            </div>*@
                                                    <!--end::Group-->
                                                    <!--begin::Group-->
                                                    <div class="form-group row">
                                                        <label class="col-xl-3 col-lg-3 col-form-label">First Name</label>
                                                        <div class="col-lg-9 col-xl-9">
                                                            <input asp-for="FirstName" class="form-control form-control-solid form-control-lg" type="text"/>
                                                        </div>
                                                    </div>
                                                    <!--end::Group-->
                                                    <!--begin::Group-->
                                                    <div class="form-group row">
                                                        <label class="col-xl-3 col-lg-3 col-form-label">Last Name</label>
                                                        <div class="col-lg-9 col-xl-9">
                                                            <input asp-for="LastName" class="form-control form-control-solid form-control-lg" type="text"/>
                                                        </div>
                                                    </div>
                                                    <!--end::Group-->
                                                    <!--begin::Group-->
                                                    <div class="form-group row">
                                                        <label class="col-xl-3 col-lg-3 col-form-label">Middle Name</label>
                                                        <div class="col-lg-9 col-xl-9">
                                                            <input asp-for="MiddleName" class="form-control form-control-solid form-control-lg" type="text"/>
                                                        </div>
                                                    </div>
                                                    <!--end::Group-->
                                                    <div class="form-group row">
                                                        <label class="col-3 col-form-label">Gender</label>
                                                        <div class="col-9 col-form-label">
                                                            <div class="radio-inline">
                                                                <label class="radio radio-outline radio-outline-2x radio-primary">
                                                                    <input type="radio" asp-for="Gender" checked="checked" value="True"/>
                                                                    <span></span>
                                                                    MALE
                                                                </label>
                                                                <label class="radio radio-outline radio-outline-2x radio-primary">
                                                                    <input type="radio" asp-for="Gender" value="False"/>
                                                                    <span></span>
                                                                    FEMALE
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <label class="col-form-label col-xl-3 col-lg-3">Birth Date</label>
                                                        <div class="col-lg-9 col-xl-9">
                                                            <div class="input-group date">
                                                                <input type="text" class="form-control" asp-for="DateOfBirth" value="@Model.DateOfBirth.ToString("MM/dd/yyyy")" readonly="readonly" placeholder="Select date"/>
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text">
                                                                        <i class="la la-calendar-check-o"></i>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--begin::Group-->
                                                    <div class="form-group row">
                                                        <label class="col-xl-3 col-lg-3 col-form-label">Address Line</label>
                                                        <div class="col-lg-9 col-xl-9">
                                                            <textarea asp-for="Address" rows="3" class="form-control form-control-solid form-control-lg"> </textarea>
                                                            <span class="form-text text-muted">Full Home address.</span>
                                                        </div>
                                                    </div>
                                                    <!--end::Group-->
                                                    <!--begin::Group-->
                                                    <div class="form-group row">
                                                        <label class="col-xl-3 col-lg-3 col-form-label">Contact Phone</label>
                                                        <div class="col-lg-9 col-xl-9">
                                                            <div class="input-group input-group-solid input-group-lg">
                                                                <div class="input-group-prepend">
                                                                    <span class="input-group-text">
                                                                        <i class="la la-phone"></i>
                                                                    </span>
                                                                </div>
                                                                <input asp-for="PhoneNumber" class="form-control form-control-solid form-control-lg" placeholder="Phone"/>
                                                            </div>
                                                            <span class="form-text text-muted">Enter valid phone number(e.g: 09123456789).</span>
                                                        </div>
                                                    </div>
                                                    <!--end::Group-->
                                                    <!--begin::Group-->
                                                    <div class="form-group row">
                                                        <label class="col-xl-3 col-lg-3 col-form-label">Email Address</label>
                                                        <div class="col-lg-9 col-xl-9">
                                                            <div class="input-group input-group-solid input-group-lg">
                                                                <div class="input-group-prepend">
                                                                    <span class="input-group-text">
                                                                        <i class="la la-at"></i>
                                                                    </span>
                                                                </div>
                                                                <input asp-for="Email" class="form-control form-control-solid form-control-lg"/>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--end::Group-->
                                                    <!--begin::Group-->
                                                    <div class="form-group row">
                                                        <label class="col-xl-3 col-lg-3 col-form-label">Weight</label>
                                                        <div class="col-lg-9 col-xl-9">
                                                            <div class="input-group input-group-solid input-group-lg">
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text text-muted">
                                                                        Kg <i class="la la-weight"></i>
                                                                    </span>
                                                                </div>
                                                                <input asp-for="Weight" type="text" class="form-control form-control-solid form-control-lg" placeholder="weight"/>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--end::Group-->
                                                    <!--begin::Group-->
                                                    <div class="form-group row">
                                                        <label class="col-xl-3 col-lg-3 col-form-label">Height</label>
                                                        <div class="col-lg-9 col-xl-9">
                                                            <div class="input-group input-group-solid input-group-lg">
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text text-muted">
                                                                        cm <i class="la la-text-height"></i>
                                                                    </span>
                                                                </div>
                                                                <input asp-for="HeightCm" type="text" class="form-control form-control-solid form-control-lg" placeholder="height"/>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--end::Group-->
                                                    <!--begin::Group-->
                                                    <div class="form-group row">
                                                        <label class="col-xl-3 col-lg-3 col-form-label">Nationality</label>
                                                        <div class="col-lg-9 col-xl-9">
                                                            <div class="input-group input-group-solid input-group-lg">
                                                                <div class="input-group-prepend">
                                                                    <span class="input-group-text">
                                                                        <i class="la la-flag-checkered"></i>
                                                                    </span>
                                                                </div>
                                                                <input asp-for="Nationality" type="text" class="form-control form-control-solid form-control-lg"/>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--end::Group-->
                                                </div>
                                                <!--end::Wizard Step 1-->
                                            </div>
                                                <div class="col-xl-10">
                                                    <!--begin::Wizard Step 3-->
                                                    <div class="my-5 step" data-wizard-type="step-content">
                                                        <!--begin::Group-->
                                                        <div class="form-group row mx-10">
                                                            <label class="col-form-label col-xl-3 col-lg-3">Course Type</label>
                                                            <div class="col-xl-9 col-lg-9">
                                                                <select onchange="KTAddStudent.getAvailableCourses(this)" asp-items="ViewBag.CourseCategories as SelectList" id="course-cat" class="form-control select2 form-control-lg form-control-solid"></select>
                                                            </div>
                                                        </div>
                                                        <!--end::Group-->
                                                        <div class="card-body" id="dvAvailableCourses">
                                                            @await Component.InvokeAsync("AvailableCourses", new { id = 1 })
                                                        </div>
                                                    </div>
                                                    <!--end::Wizard Step 3-->
                                                </div>
                                                <div class="col-xl-12 ">
                                                    <!--begin::Wizard Step 2-->
                                                    <div class="my-5 step" data-wizard-type="step-content">
                                                        <!--begin::Invoice-->
                                                        <div class="row justify-content-center px-8 px-md-0 invoice" id="invoice">
                                                            <div class="col-md-12">
                                                                <!-- begin: Invoice header-->
                                                                <div class="d-flex justify-content-between pb-10 pb-md-20 flex-column flex-md-row border-bottom">
                                                                    <h1 class="display-4 font-weight-boldest mb-10">INVOICE</h1>
                                                                    <div class="d-flex flex-column align-items-md-end px-0">
                                                                        <!--begin::Logo-->
                                                                        <a href="#" class="mb-5 max-w-200px">
                                                                            <img alt="AJ3 Universal" height="50" src="assets/media/logos/AJ3.jpg" />
                                                                        </a>
                                                                        <!--end::Logo-->
                                                                        <span class="d-flex flex-column align-items-md-end font-size-h5 font-weight-bold text-muted">
                                                                            <span>@comProf.Name</span>
                                                                            <span class="font-size-base ">@comProf.Address</span>
                                                                            <span class="font-size-base font-weight-light">Mobile: @comProf.MobileNumber @(!string.IsNullOrEmpty(comProf.PhoneNumber) ? $"Telephone:{comProf.PhoneNumber}" : "")</span>
                                                                        </span>
                                                                    </div>
                                                                </div>

                                                                <!--end: Invoice header-->
                                                                <!--begin: Invoice body-->
                                                                <div class="row border-bottom pb-10">
                                                                    <div class="col-md-9 py-md-10 pr-md-10">
                                                                        <div class="table-responsive">
                                                                            <table class="table">
                                                                                <thead>
                                                                                    <tr>
                                                                                        <th class="pt-1 pb-9 pl-0 font-weight-bolder text-muted font-size-lg text-uppercase">Description</th>
                                                                                        <th class="pt-1 pb-9 text-right font-weight-bolder text-muted font-size-lg text-uppercase">Hours</th>
                                                                                        <th class="pt-1 pb-9 text-right font-weight-bolder text-muted font-size-lg text-uppercase">Rate</th>
                                                                                        <th class="pt-1 pb-9 text-right pr-0 font-weight-bolder text-muted font-size-lg text-uppercase">Amount</th>
                                                                                    </tr>
                                                                                </thead>
                                                                                <tbody>
                                                                                    <tr class="font-weight-bolder font-size-lg">
                                                                                        <td class="border-top-0 pl-0 pt-7 d-flex align-items-center">
                                                                                            <span class="navi-icon mr-2">
                                                                                                <i class="fa fa-certificate text-hover-success font-size-h2"></i>
                                                                                            </span><span id="iv-desc"></span>
                                                                                        </td>
                                                                                        <td class="text-right pt-7">
                                                                                            <span id="iv-hours"></span>
                                                                                        </td>
                                                                                        <td class="text-right pt-7">
                                                                                            <span id="iv-unitprice"></span>
                                                                                        </td>
                                                                                        <td class="pr-0 pt-7 font-size-h6 font-weight-boldest text-right">
                                                                                            <span id="iv-amount"></span>
                                                                                        </td>
                                                                                    </tr>
                                                                                    <tr class="font-weight-bolder font-size-lg">
                                                                                        <td class="border-top-0 pl-0 pt-7 d-flex align-items-center">
                                                                                            <span class="navi-icon mr-2">
                                                                                                <i class="fa fa-percent text-hover-danger font-size-h2"></i>
                                                                                            </span>
                                                                                            Discount
                                                                                        </td>
                                                                                        <td class="text-right pt-7">-</td>
                                                                                        <td class="text-right pt-7">-</td>
                                                                                        <td class="pr-0 pt-7 font-size-h6 font-weight-boldest text-right">
                                                                                            <span id="iv-discount"></span>
                                                                                        </td>
                                                                                    </tr>
                                                                                </tbody>
                                                                            </table>
                                                                        </div>
                                                                        <div class="border-bottom w-100 mt-7 mb-11"></div>
                                                                        <div class="d-flex flex-column flex-md-row">
                                                                            <div class="d-flex flex-column mb-10 mb-md-0">
                                                                                <div class="font-weight-bold font-size-h6 mb-3">PAYMENT DETAIL</div>
                                                                                <div class="form-group row">
                                                                                    <label class="col-xl-4 col-md-4 col-lg-4 col-form-label text-right">OR NUMBER:</label>
                                                                                    <div class="col-lg-8 col-md-8 col-xl-8">
                                                                                        <div class="input-group input-group-solid input-group-lg">
                                                                                            <input asp-for="OrNumber" type="text" class="form-control form-control-solid text-right form-control-lg" placeholder="OR Number" />
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="form-group row">
                                                                                    <label class="col-xl-4 col-md-4 col-lg-4 col-form-label text-right">AMOUNT:</label>
                                                                                    <div class="col-lg-8 col-md-8 col-xl-8">
                                                                                        <div class="input-group  input-group-lg text-right ">
                                                                                            <input id="PayAmountText" onfocusout="KTAddStudent.formatNumber(this)" onfocusin="KTAddStudent.onFocus(this)"
                                                                                                   onkeyup="KTAddStudent.computeBalance(this)" type="text" class="form-control text-right font-weight-bold text-danger form-control-solid form-control-lg" placeholder="" value="0" />
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="form-group row">
                                                                                    <label class="col-xl-4 col-md-4 col-lg-4 col-form-label text-right">BALANCE:</label>
                                                                                    <div class="col-lg-8 col-md-8 col-xl-8">
                                                                                        <div class="input-group  input-group-lg text-right ">
                                                                                            <input id="ivBalance" type="text" disabled class="form-control text-right font-weight-bold text-danger form-control-solid form-control-lg" placeholder="" value="0" />
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-md-3 border-left-md pl-md-10 py-md-10 text-right">
                                                                        <!--begin::Total Amount-->
                                                                        <div class="font-size-h4 font-weight-bolder text-muted mb-3">TOTAL AMOUNT</div>
                                                                        <div class="font-size-h1 font-weight-boldest">
                                                                            <span id="iv-totalamount"></span>
                                                                        </div>
                                                                        <div class="text-muted font-weight-bold mb-16">Taxes included</div>
                                                                        <!--end::Total Amount-->
                                                                        <div class="border-bottom w-100 mb-16"></div>
                                                                        <!--begin::Invoice To-->
                                                                        <div class="text-dark-50 font-size-lg font-weight-bold mb-3">INVOICE TO.</div>
                                                                        <div class="font-size-lg font-weight-bold mb-10">
                                                                            <span id="iv-to"></span>
                                                                            <br />
                                                                            <span id="iv-address"></span>
                                                                        </div>
                                                                        <!--end::Invoice To-->
                                                                        <!--begin::Invoice No-->
                                                                        <div class="text-dark-50 font-size-lg font-weight-bold mb-3">INVOICE NO.</div>
                                                                        <div class="font-size-lg font-weight-bold mb-10">
                                                                            <span>@ViewBag.OrderNumber</span>
                                                                        </div>
                                                                        <!--end::Invoice No-->
                                                                        <!--begin::Invoice Date-->
                                                                        <div class="text-dark-50 font-size-lg font-weight-bold mb-3">DATE</div>
                                                                        <div class="font-size-lg font-weight-bold">@DateTime.Now.ToString("D")</div>
                                                                        <!--end::Invoice Date-->
                                                                    </div>
                                                                </div>
                                                                <!--end: Invoice body-->
                                                            </div>
                                                        </div>
                                                        <!-- begin: Invoice action-->
                                                        <!--begin::Group-->
                                                        @*<div class="form-group row my-5">
                                                                <div class="col-xl-9 col-lg-9">
                                                                    <button type="button" onclick="KTAddStudent.printInvoice()" class="btn btn-light-success font-weight-bold btn-sm">Print Invoice</button>
                                                                </div>
                                                            </div>*@
                                                        <!--end::Group-->
                                                    </div>
                                                    <!--end::Wizard Step 2-->
                                                </div>
                                                <div class="col-xl-10">
                                                    <!--begin::Wizard Step 4-->
                                                    <div class="my-5 step" data-wizard-type="step-content">
                                                        <h5 class="mb-10 font-weight-bold text-dark">Review Student Details and Submit</h5>
                                                        <!--begin::Item-->
                                                        <div class="border-bottom mb-5 pb-5">
                                                            <div class="font-weight-bolder mb-3">Student Profile Details:</div>
                                                            <div class="line-height-xl">
                                                                <span id="sd-fullname"></span>
                                                                <br />Gender: <span id="sd-gender"></span>
                                                                <br />Height(cm): <span id="sd-height"></span>
                                                                <br />Weight: <span id="sd-weight"></span>
                                                                <br />Nationality: <span id="sd-nationality"></span>
                                                                <br />Phone: <span id="sd-phone"></span>
                                                                <br />Email: <span id="sd-email"></span>
                                                            </div>
                                                        </div>
                                                        <!--end::Item-->
                                                        <!--begin::Item-->
                                                        <div class="border-bottom mb-5 pb-5">
                                                            <div class="font-weight-bolder mb-3">Address Details:</div>
                                                            <div class="line-height-xl">
                                                                <span id="sd-address"></span>
                                                                <br />Philippiness
                                                            </div>
                                                        </div>
                                                        <!--end::Item-->
                                                        <!--begin::Item-->
                                                        <div class="border-bottom mb-5 pb-5">
                                                            <div class="font-weight-bolder">Course Details:</div>
                                                            <div class="line-height-xl">
                                                                <span id="sd-coursedesc"></span>
                                                                <br />Hours: <span id="sd-hours"></span>
                                                            </div>
                                                        </div>
                                                        <!--end::Item-->
                                                        <!--begin::Item-->
                                                        <div>
                                                            <div class="font-weight-bolder">Payment Details:</div>
                                                            <div class="line-height-xl">
                                                                Total Amount: <span id="sd-totalamount"></span>
                                                                <br />Total Discount: <span id="sd-totaldiscount"></span>
                                                                <br />Total Payment: <span id="sd-totalpayment"></span>
                                                                <br />Total Balance: <span id="sd-balance"></span>
                                                            </div>
                                                        </div>
                                                        <!--end::Item-->
                                                    </div>
                                                    <!--end::Wizard Step 4-->
                                                </div>
                                                <div class="col-xl-10">
                                                    <!--begin::Wizard Actions-->
